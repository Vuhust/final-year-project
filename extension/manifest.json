  "version": "1.1",
  "permissions": [ "storage", "tabs", "privacy"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": ["scripts/libs/jquery.js","scripts/libs/CryptoJS.js","scripts/content.js"],
      "matches": ["<all_urls>" ]
    }
  ]

}
